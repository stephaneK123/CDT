n          if (!value) return;\r\n          addTodo(value);\r\n          setValue(\"\");\r\n        };\r\n      \r\n        return (\r\n          <Form onSubmit={handleSubmit}> \r\n          \r\n             <Form.Group>\r\n                 <Form.Control style={{ height: '20px' }}type=\"text\" className=\"input\" value={value}   onChange={e => setValue(e.target.value)} placeholder=\"Add new todo\" />\r\n             </Form.Group>\r\n           \r\n            <Form.Group>\r\n                <Button type=\"submit\">=Submit</Button> \r\n            </Form.Group>\r\n          </Form>\r\n        );\r\n      }\r\n\r\n    return (\r\n        <div style={{ borderTop: '1px solid #f0f0f0' }} className='ce-photo-section'>\r\n            <SettingsBlock\r\n                label='Lists'\r\n                className='ce-section-title-container ce-photo-title-container'\r\n            >\r\n\r\n<div className=\"container\">\r\n        <FormTodo addTodo={addTodo} />\r\n        <div>\r\n          {todos.map((todo, index) => (\r\n            <Card>\r\n              <Card.Body>\r\n                <Todo\r\n                key={index}\r\n                index={index}\r\n                todo={todo}\r\n                markTodo={markTodo}\r\n                removeTodo={removeTodo}\r\n                />\r\n              </Card.Body>\r\n            </Card>\r\n          ))}\r\n        </div>\r\n      </div>\r\n\r\n            </SettingsBlock>\r\n        </div>\r\n    )\r\n}\r\n\r\n\r\n\r\n\r\nexport default ListsSettings","C:\\Users\\phill\\Dropbox\\PC\\Documents\\GitHub\\CDT\\src\\components\\ChatSettings\\Poll\\SettingsBlock.js",[],"C:\\Users\\phill\\Dropbox\\PC\\Documents\\GitHub\\CDT\\src\\components\\ChatSettings\\Poll\\Thumbnail.js",[],{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"107","replacedBy":"111"},{"ruleId":"109","replacedBy":"112"},{"ruleId":"113","severity":1,"message":"114","line":4,"column":21,"nodeType":"115","messageId":"116","endLine":4